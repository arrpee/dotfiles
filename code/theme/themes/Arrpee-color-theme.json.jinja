			"settings": {
				"foreground": "#BBBBBB"
			}
		},
		{
			"name": "Tag brackets",
			"scope": "punctuation.definition.tag",
			"settings": {
				"foreground": "{{ mappings['xr_color1'] }}B3",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Key/value separators",
