			"name": "Storage keywords",
			"scope": "storage",
			"settings": {
				"foreground": "{{ mappings['xr_color5'] }}E6",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string.quoted",
				"string.template"
			],
			"settings": {
				"foreground": "{{ mappings['xr_color3'] }}E6"
			}
		},
		{
			"name": "Regex",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#F09483E6"
			}
		},
		{
			"name": "Markup link descriptions",
			"scope": "string.other.link",
			"settings": {
				"foreground": "#F09483E6"
			}
		},
		{
			"name": "Provided (support) values",
			"scope": "support",
			"settings": {
				"foreground": "#FAC29AE6"
			}
		},
		{
			"name": "Provided functions",
			"scope": "support.function",
			"settings": {
				"foreground": "{{ mappings['xr_color4'] }}E6"
			}
		},
		{
			"name": "Provieded variables",
			"scope": "support.variable",
			"settings": {
				"foreground": "{{ mappings['xr_color1'] }}E6"
			}
		},
		{
			"name": "Object/JSON property names",
			"scope": [
				"support.type.property-name",
