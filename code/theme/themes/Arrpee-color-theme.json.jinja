			],
			"settings": {
				"foreground": "{{ mappings['xr_color1'] }}E6"
			}
		},
		{
			"name": "CSS property names",
			"scope": "support.type.property-name.css",
			"settings": {
				"foreground": "#BBBBBB"
			}
